<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#d68402"></solid>
            <corners android:topLeftRadius="@dimen/size10"
                android:topRightRadius="@dimen/size10"></corners>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/bulk_orders"></solid>
            <corners android:topLeftRadius="@dimen/size10"
                android:topRightRadius="@dimen/size10"></corners>
        </shape>
    </item>
</selector>