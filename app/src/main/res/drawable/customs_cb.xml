<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_selected="true">
        <inset
            android:drawable="@mipmap/icon_cb_checked"></inset>
    </item>
    <item
        android:state_selected="false">
        <inset
            android:drawable="@drawable/background_checkbox_uncheck"></inset>
    </item>
    <item
        android:state_enabled="true">
        <inset android:drawable="@mipmap/icon_cb_enable"></inset>
    </item>
</selector>